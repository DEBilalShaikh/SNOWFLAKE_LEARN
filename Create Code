USE ROLE SYSADMIN

CREATE OR REPLACE WAREHOUSE SEC_LEARN

WITH
WAREHOUSE_SIZE = XSMALL
MIN_CLUSTER_COUNT = 1
MAX_CLUSTER_COUNT = 2
INITIALLY_SUSPENDED = true
Auto_suspend = 600
COMMENT = "THIS IS OUR SECOND WAREHOSUE"


ALTER WAREHOUSE SEC_LEARN RESUME

ALTER WAREHOUSE SEC_LEARN SUSPEND

ALTER WAREHOUSE SEC_LEARN SET WAREHOUSE_SIZE = 'X-SMALL'
